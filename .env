VITE_BASE_URL=/

VITE_APP_TITLE=ThingsPanel

VITE_APP_DESC=ThingsPanel

# the prefix of the icon name
VITE_ICON_PREFIX=icon

# the prefix of the local svg icon component, must include VITE_ICON_PREFIX
# format {VITE_ICON_PREFIX}-{local icon name}
VITE_ICON_LOCAL_PREFIX=icon-local

# auth route mode: static ï½œ dynamic
# VITE_AUTH_ROUTE_MODE=static
VITE_AUTH_ROUTE_MODE=dynamic
# static auth route home
VITE_ROUTE_HOME=home

# default menu icon
VITE_MENU_ICON=mdi:menu

# encrypt password
VITE_ENCRYPT_PASSWORD=0

# ThingsVis Studio URL (used for embedded visualization editor)
# Production: set the actual ThingsVis Studio URL
# VITE_THINGSVIS_STUDIO_URL=https://your-domain.com/thingsvis#/editor
