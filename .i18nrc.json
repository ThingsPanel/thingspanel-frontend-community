{
  "localesPaths": ["src/locales/langs"],
  "pathMatcher": "{locale}.ts",
  "namespace": false,
  "keystyle": "nested",
  "sourceLanguage": "zh-CN",
  "displayLanguage": "zh-CN",
  "enabledParsers": ["ts"],
  "enabledFrameworks": ["vue", "vue-i18n"],
  "parsers": {
    "ts": {
      "compilerOptions": {
        "baseUrl": "./",
        "paths": {
          "@/*": ["src/*"]
        }
      }
    }
  }
} 